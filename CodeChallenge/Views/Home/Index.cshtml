@{
    ViewBag.Title = "Home Page";
}

<div id="ErrorMsg" style="display:none" class="alert alert-danger">
    <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
    <strong>Error!</strong> Something went wrong please try again.
</div>

<div class="uploadForm">

        <div class="offset-2 col-8">
            <div class="col-12">
                <h1 class="text-center OswaldTypeBold">Please upload HTML file</h1>
            </div>
            <div class="col-12">
                <h5 style="margin-top:10px" class="text-center OswaldTypeExtraLight">to retrieve the data</h5>
            </div>
            <div class="offset-4 col-4 customMargin">
                <span class="btn btn-success btn-block btn-file ">
                    Upload Here! <input id="uploadFile" accept="text/html" type="file">
                </span>
            </div>
        </div>
</div>



    @Html.Partial("_Modal")

